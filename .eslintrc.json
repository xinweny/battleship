{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true
    },
    "plugins": ["jest"],
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "parser": "@babel/eslint-parser",
    "rules": {
        "class-methods-use-this": "off",
        "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"]
    }
}
